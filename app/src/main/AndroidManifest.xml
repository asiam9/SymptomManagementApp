<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.skywomantech.app.symptommanagement" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_SYNC_SETTINGS" />
    <uses-permission android:name="android.permission.WRITE_SYNC_SETTINGS" />
    <uses-permission android:name="android.permission.AUTHENTICATE_ACCOUNTS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-feature android:name="android.hardware.camera" android:required="false" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name=".data.PatientContentProvider"
            android:authorities="@string/content_authority"
            android:exported="false"
            android:syncable="true" />

        <activity
            android:name=".SetPreferenceActivity"
            android:label="@string/action_settings"
            android:parentActivityName=".Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.skywomantech.app.symptommanagement.Login" />
        </activity>
        <activity
            android:name=".admin.AdminMain"
            android:label="@string/title_activity_admin_main"
            android:parentActivityName=".Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.skywomantech.app.symptommanagement.Login" />
        </activity>
        <activity
            android:name=".admin.Patient.AdminPatientListActivity"
            android:label="@string/title_admin_patient_list"
            android:parentActivityName=".admin.AdminMain" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.skywomantech.app.symptommanagement.AdminMain" />
        </activity>
        <activity
            android:name=".admin.Patient.AdminPatientDetailActivity"
            android:label="@string/title_admin_patient_detail"
            android:parentActivityName=".admin.Patient.AdminPatientListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.skywomantech.app.symptommanagement.AdminPatientListActivity" />
        </activity>
        <activity
            android:name=".admin.Physician.AdminPhysicianListActivity"
            android:label="@string/title_admin_physician_list"
            android:parentActivityName=".admin.AdminMain" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.skywomantech.app.symptommanagement.AdminMain" />
        </activity>
        <activity
            android:name=".admin.Patient.PatientPhysicianListActivity"
            android:label="@string/title_admin_physician_list"
            android:parentActivityName=".admin.Patient.AdminPatientDetailActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.skywomantech.app.symptommanagement.AdminMain" />
        </activity>
        <activity
            android:name=".admin.Physician.AdminPhysicianDetailActivity"
            android:label="@string/title_admin_physician_detail"
            android:parentActivityName=".admin.Physician.AdminPhysicianListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.skywomantech.app.symptommanagement.AdminPhysicianListActivity" />
        </activity>
        <activity
            android:name=".admin.Medication.AdminMedicationListActivity"
            android:label="@string/title_admin_medication_list"
            android:parentActivityName=".admin.AdminMain" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.skywomantech.app.symptommanagement.AdminMain" />
        </activity>
        <activity
            android:name=".admin.Medication.AdminMedicationDetailActivity"
            android:label="@string/title_admin_medication_detail"
            android:parentActivityName=".admin.Medication.AdminMedicationListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.skywomantech.app.symptommanagement.adminmedicationListActivity" />
        </activity>

        <activity
            android:name=".patient.PatientMainActivity"
            android:label="@string/title_activity_patient_main"
            android:parentActivityName=".Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.skywomantech.app.symptommanagement.Login" />
        </activity>
        <activity
            android:name=".physician.PhysicianPatientListActivity"
            android:label="@string/title_physicianpatient_list"
            android:parentActivityName=".Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.skywomantech.app.symptommanagement.Login" />
        </activity>
        <activity
            android:name=".physician.PhysicianPatientDetailActivity"
            android:label="@string/title_physicianpatient_detail"
            android:parentActivityName=".physician.PhysicianPatientListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.skywomantech.app.symptommanagement.physician.PhysicianPatientListActivity" />
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="Sign In"
            android:parentActivityName=".Login"
            android:windowSoftInputMode="adjustResize|stateVisible" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.skywomantech.app.symptommanagement.Login" />
        </activity>

        <provider
            android:authorities="@string/content_authority"
            android:name=".data.PatientContentProvider"
            android:exported="false"
            android:syncable="true" />

    </application>

</manifest>
